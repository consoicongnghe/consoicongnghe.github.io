{
	"sitemap.xml": {
		"Protocol": "https",
		"DomainName": "consoicongnghe.com",
		"Root": "./",
		"IncludeExt": [
			".html",
			".php"
		],
		"Exclude": [
			"cgi-bin/*",
			".github/*",
			".vscode/*",
			"tdp/*",
			"tools/*",
			"test/*",
			"404.html",
			"readme.md",
			"blog/archived/*",
			"blog/private/*",
			"assets/*"
		],
		"TagsToInclude": [
			"priority",
			"lastmod"
		],
		"DefaultChangeFrequency": "weekly",
		"TabCharacters": "\t",
		"bIncludeWWW": false,
		"bRemoveFileExtentions": false,
		"bUseTrailingSlash": true,
		"bAutomaticallyUpdateSitemap": true,
		"bMinimized": false
	}
}